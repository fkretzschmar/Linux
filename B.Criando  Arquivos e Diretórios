#Criar diretório dentro de labs
mkdir arqs_dirs


#Entrar dendro do diretório criado
cd arqs_dirs/


#criar varios diretórios um dentro do outro
mkdir -p dir1/dir2/dir3/dir4/


#Argumentos(comandos) mkdir
mkdir --help


#Criar Arquivos
touch arq1


#Criar um diretório contendo espaço no nome do mesmo
mkdir diretorio\ 1
